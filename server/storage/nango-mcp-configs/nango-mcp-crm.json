{
  "mcpServers": {
    "salesforce": {
      "type": "stdio",
      "command": "node",
      "args": [
        "/Users/segevbin/anything-llm/server/nango-template-mcp-wrapper.js",
        "salesforce"
      ],
      "env": {
        "NANGO_SECRET_KEY": "${NANGO_SECRET_KEY}",
        "NANGO_HOST": "${NANGO_HOST}",
        "NANGO_PROVIDER_CONFIG_KEY": "salesforce-getting-started",
        "MCP_SERVICE_NAME": "salesforce",
        "WORKSPACE_ID": "${WORKSPACE_ID}"
      },
      "anythingllm": {
        "autoStart": true,
        "workspaceAware": true,
        "description": "salesforce integration via Nango templates",
        "toolPrefix": "salesforce",
        "priority": "high",
        "category": "crm",
        "capabilities": {
          "oauth": true,
          "api": true,
          "realtime": false,
          "webhooks": true
        }
      }
