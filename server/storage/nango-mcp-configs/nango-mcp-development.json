{
  "mcpServers": {
    "github": {
      "type": "stdio",
      "command": "node",
      "args": [
        "/Users/segevbin/anything-llm/server/nango-template-mcp-wrapper.js",
        "github"
      ],
      "env": {
        "NANGO_SECRET_KEY": "${NANGO_SECRET_KEY}",
        "NANGO_HOST": "${NANGO_HOST}",
        "NANGO_PROVIDER_CONFIG_KEY": "github-getting-started",
        "MCP_SERVICE_NAME": "github",
        "WORKSPACE_ID": "${WORKSPACE_ID}"
      },
      "anythingllm": {
        "autoStart": true,
        "workspaceAware": true,
        "description": "github integration via Nango templates",
        "toolPrefix": "github",
        "priority": "high",
        "category": "development",
        "capabilities": {
          "oauth": true,
          "api": true,
          "realtime": false,
