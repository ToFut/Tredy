{
  "mcpServers": {
    "shopify": {
      "type": "stdio",
      "command": "node",
      "args": [
        "/Users/segevbin/anything-llm/server/nango-template-mcp-wrapper.js",
        "shopify"
      ],
      "env": {
        "NANGO_SECRET_KEY": "${NANGO_SECRET_KEY}",
        "NANGO_HOST": "${NANGO_HOST}",
        "NANGO_PROVIDER_CONFIG_KEY": "shopify-getting-started",
        "MCP_SERVICE_NAME": "shopify",
        "WORKSPACE_ID": "${WORKSPACE_ID}"
      },
      "anythingllm": {
        "autoStart": true,
        "workspaceAware": true,
        "description": "shopify integration via Nango templates",
        "toolPrefix": "shopify",
        "priority": "high",
        "category": "ecommerce",
        "capabilities": {
          "oauth": true,
          "api": true,
          "realtime": false,
          "webhooks": true
        }
      }
    },
