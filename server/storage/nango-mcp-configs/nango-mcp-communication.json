{
  "mcpServers": {
    "slack": {
      "type": "stdio",
      "command": "node",
      "args": [
        "/Users/segevbin/anything-llm/server/nango-template-mcp-wrapper.js",
        "slack"
      ],
      "env": {
        "NANGO_SECRET_KEY": "${NANGO_SECRET_KEY}",
        "NANGO_HOST": "${NANGO_HOST}",
        "NANGO_PROVIDER_CONFIG_KEY": "slack-getting-started",
        "MCP_SERVICE_NAME": "slack",
        "WORKSPACE_ID": "${WORKSPACE_ID}"
      },
      "anythingllm": {
        "autoStart": true,
        "workspaceAware": true,
        "description": "slack integration via Nango templates",
        "toolPrefix": "slack",
        "priority": "high",
        "category": "communication",
        "capabilities": {
          "oauth": true,
          "api": true,
          "realtime": true,
          "webhooks": true
        }
      }
    },
